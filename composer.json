{
  "name": "davidbarratt/davidwbarratt",
  "description": "David W Barratt",
  "type": "project",
  "license": "AGPL-3.0",
  "require": {
    "composer/installers": "^1.0.20",
    "drupal/core": "^8.7.0",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/imagemagick": "^3.1",
    "drupal/console": "^1.2.0",
    "drupal/swiftmailer": "^2.0",
    "drupal/schema_metatag": "^1.4",
    "drupal/name": "^1.0@RC",
    "cweagans/composer-patches": "^1.6",
    "drupal/upgrade_status": "^2.0",
    "drush/drush": "^10.2.2"
  },
  "replace": {
    "drupal/drupal": "~8.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Drupal8",
    "optimize-autoloader": true
  },
  "extra": {
    "patches": {
      "drupal/schema_metatag": {
        "Add @id as a tag to all schemas": "https://www.drupal.org/files/issues/2019-09-01/3078582-03.patch"
      }
    },
    "installer-paths": {
      "html/core": ["type:drupal-core"],
      "html/modules/contrib/{$name}": ["type:drupal-module"],
      "html/themes/contrib/{$name}": ["type:drupal-theme"],
      "html/profiles/contrib/{$name}": ["type:drupal-profile"]
    }
  },
  "require-dev": {
    "symfony/var-dumper": "^3.4"
  }
}
